{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "html"
  ],
  "globals": {
    "DGTElement": true
  },
  "rules": {
    "indent": [
      "error"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}